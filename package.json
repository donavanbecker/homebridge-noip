{
  "displayName": "No-IP",
  "name": "homebridge-noip",
  "version": "2.0.3",
  "description": "The [Homebridge](https://homebridge.io) No-IP plugin allows you to update your [No-IP](http://noip.com) hostname(s) for your homebridge instance.",
  "author": "donavanbecker",
  "license": "Apache-2.0",
  "icon": "https://raw.githubusercontent.com/donavanbecker/homebridge-noip/beta-2.0.3/branding/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/donavanbecker/homebridge-noip.git"
  },
  "bugs": {
    "url": "https://github.com/donavanbecker/homebridge-noip/issues"
  },
  "engines": {
    "homebridge": "^1.7.0",
    "node": "^18 || ^20"
  },
  "main": "dist/index.js",
  "scripts": {
    "check": "npm install && npm outdated",
    "update": "ncu -u && npm update && npm install",
    "update dependencies": "npm run check && npm run update",
    "lint": "eslint src/**.ts",
    "watch": "npm run build && npm link && nodemon",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run update && npm run lint && npm run build",
    "postpublish": "npm run clean",
    "clean": "rimraf ./dist",
    "test": "eslint src/**.ts"
  },
  "funding": [
    {
      "type": "Paypal",
      "url": "https://www.paypal.me/donavanbecker"
    },
    {
      "type": "GitHub",
      "url": "https://github.com/sponsors/donavanbecker"
    }
  ],
  "keywords": [
    "homebridge-plugin",
    "noip",
    "No-IP"
  ],
  "dependencies": {
    "@homebridge/plugin-ui-utils": "^1.0.0",
    "undici": "^5.28.0",
    "rxjs": "^7.8.1",
    "super-stringify": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "nodemon": "^3.0.1",
    "homebridge": "^1.7.0",
    "npm-check-updates": "^16.14.11",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
